<root>

	<!-- WEBSTORM SHORTCUT -->
	<vkopenurldef>
  	<name>KeyCode::VK_OPEN_URL_APP_WebStorm</name>
  	<url type="file">/Applications/WebStorm.app</url>
	</vkopenurldef>

	<item>
  	<name>Ctrl-Alt-W = WebStorm</name>
  	<identifier>private.ctrl_alt_w</identifier>
    <autogen>
      __KeyToKey__
      KeyCode::W, ModifierFlag::CONTROL_L,ModifierFlag::COMMAND_L,
      KeyCode::VK_OPEN_URL_APP_WebStorm,
    </autogen>
	</item>

